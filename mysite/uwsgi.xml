<uwsgi>  
    <socket>0.0.0.0:7321</socket>  
    <master>true</master>  
    <pidfile>../uwsgi.pid</pidfile>  
    <processes>2</processes>  
    <module>mysite/wsgi</module>  
    <wsgi-file>mysite/wsgi.py</wsgi-file>
    <profiler>true</profiler>  
